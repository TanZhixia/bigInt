#ifndef BIGINT_H
#define BIGINT_H
#include <istream>
#include <ostream>
#include <string>
class BigInt {
    private:
        std::string num;
    public:
        void correct();
        BigInt();
        BigInt(const std::string& s);
        BigInt(const char* s);
        BigInt(const BigInt& s);
        BigInt(const int& s);
        BigInt(const long& s);
        BigInt(const long long& s);
        BigInt(const unsigned int& s);
        BigInt(const unsigned long& s);
        BigInt(const unsigned long long& s);
        operator std::string() const;
        operator const char*() const;
        operator int() const;
        operator long() const;
        operator long long() const;
        operator unsigned int() const;
        operator unsigned long() const;
        operator unsigned long long() const;
        BigInt& operator=(const std::string& s);
        BigInt& operator=(const char* s);
        BigInt& operator=(const BigInt& s);
        BigInt& operator=(const int& s);
        BigInt& operator=(const long& s);
        BigInt& operator=(const long long& s);
        BigInt& operator=(const unsigned int& s);
        BigInt& operator=(const unsigned long& s);
        BigInt& operator=(const unsigned long long& s);
        bool operator==(const std::string& s) const;
        bool operator==(const char* s) const;
        bool operator==(const BigInt& s) const;
        bool operator==(const int& s) const;
        bool operator==(const long& s) const;
        bool operator==(const long long& s) const;
        bool operator==(const unsigned int& s) const;
        bool operator==(const unsigned long& s) const;
        bool operator==(const unsigned long long& s) const;
        bool operator!=(const std::string& s) const;
        bool operator!=(const char* s) const;
        bool operator!=(const BigInt& s) const;
        bool operator!=(const int& s) const;
        bool operator!=(const long& s) const;
        bool operator!=(const long long& s) const;
        bool operator!=(const unsigned int& s) const;
        bool operator!=(const unsigned long& s) const;
        bool operator!=(const unsigned long long& s) const;
        bool operator<(const std::string& s) const;
        bool operator<(const char* s) const;
        bool operator<(const BigInt& s) const;
        bool operator<(const int& s) const;
        bool operator<(const long& s) const;
        bool operator<(const long long& s) const;
        bool operator<(const unsigned int& s) const;
        bool operator<(const unsigned long& s) const;
        bool operator<(const unsigned long long& s) const;
        bool operator<=(const std::string& s) const;
        bool operator<=(const char* s) const;
        bool operator<=(const BigInt& s) const;
        bool operator<=(const int& s) const;
        bool operator<=(const long& s) const;
        bool operator<=(const long long& s) const;
        bool operator<=(const unsigned int& s) const;
        bool operator<=(const unsigned long& s) const;
        bool operator<=(const unsigned long long& s) const;
        bool operator>(const std::string& s) const;
        bool operator>(const char* s) const;
        bool operator>(const BigInt& s) const;
        bool operator>(const int& s) const;
        bool operator>(const long& s) const;
        bool operator>(const long long& s) const;
        bool operator>(const unsigned int& s) const;
        bool operator>(const unsigned long& s) const;
        bool operator>(const unsigned long long& s) const;
        bool operator>=(const std::string& s) const;
        bool operator>=(const char* s) const;
        bool operator>=(const BigInt& s) const;
        bool operator>=(const int& s) const;
        bool operator>=(const long& s) const;
        bool operator>=(const long long& s) const;
        bool operator>=(const unsigned int& s) const;
        bool operator>=(const unsigned long& s) const;
        bool operator>=(const unsigned long long& s) const;
        BigInt operator+(const std::string& s) const;
        BigInt operator+(const char* s) const;
        BigInt operator+(const BigInt& s) const;
        BigInt operator+(const int& s) const;
        BigInt operator+(const long& s) const;
        BigInt operator+(const long long& s) const;
        BigInt operator+(const unsigned int& s) const;
        BigInt operator+(const unsigned long& s) const;
        BigInt operator+(const unsigned long long& s) const;
        BigInt operator-(const std::string& s) const;
        BigInt operator-(const char* s) const;
        BigInt operator-(const BigInt& s) const;
        BigInt operator-(const int& s) const;
        BigInt operator-(const long& s) const;
        BigInt operator-(const long long& s) const;
        BigInt operator-(const unsigned int& s) const;
        BigInt operator-(const unsigned long& s) const;
        BigInt operator-(const unsigned long long& s) const;
        friend std::ostream& operator<<(std::ostream& os, const BigInt& s);
        friend std::istream& operator>>(std::istream& is, BigInt& s);
};
BigInt operator+(const std::string& s, const BigInt& other);
BigInt operator+(const char* s, const BigInt& other);
BigInt operator+(const int& s, const BigInt& other);
BigInt operator+(const long& s, const BigInt& other);
BigInt operator+(const long long& s, const BigInt& other);
BigInt operator+(const unsigned int& s, const BigInt& other);
BigInt operator+(const unsigned long& s, const BigInt& other);
BigInt operator+(const unsigned long long& s, const BigInt& other);
BigInt operator-(const std::string& s, const BigInt& other);
BigInt operator-(const char* s, const BigInt& other);
BigInt operator-(const int& s, const BigInt& other);
BigInt operator-(const long& s, const BigInt& other);
BigInt operator-(const long long& s, const BigInt& other);
BigInt operator-(const unsigned int& s, const BigInt& other);
BigInt operator-(const unsigned long& s, const BigInt& other);
BigInt operator-(const unsigned long long& s, const BigInt& other);
#endif